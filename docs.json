{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Rabfy",
  "appearance": {
    "strict": false,
    "default": "system"
  },
  "colors": {
    "primary": "#e5662f",
    "light": "#e5662f",
    "dark": "#e5662f"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
